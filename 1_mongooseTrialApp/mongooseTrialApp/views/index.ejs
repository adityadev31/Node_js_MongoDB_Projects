  <!-- TITLE -->
  <title><%= title %></title>
</head>
<body>
  <h1><%= title %></h1>


  <!-- TABLE -->
  <table class="table">
    <thead class="thead-dark">
      <tr>
        <th scope="col">Name</th>
        <th scope="col">Email</th>
        <th scope="col">Hourly Rate</th>
        <th scope="col">Total Hrs</th>
        <th scope="col">Total Amount</th>
      </tr>
    </thead>
    <tbody>
      <% records.forEach(function(row){ %>
      <tr>
        <td><%= row.name %></td>
        <td><%= row.email %></td>
        <td><%= row.hourlyRate %></td>
        <td><%= row.totalHour %></td>
        <td><%= row.total %></td>
      </tr>
      <% }) %>
    </tbody>
  </table>
  


</body>
